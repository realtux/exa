{
    "type": "module",
    "imports": {
        "#app/*.js": "./*.js"
    },
    "dependencies": {
        "@exajs/core": "../..",
        "nodemon": "2.0.4"
    },
    "scripts": {
        "watch": "nodemon -q --watch ../.. --ignore node_modules/ -e '*' -x '%%RUNTIME%% app.js || touch app.js'",
        "start": "%%RUNTIME%% app.js",
        "console": "%%RUNTIME%% app.js console $*"
    }
}
